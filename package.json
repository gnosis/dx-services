{
  "name": "@gnosis.pm/dx-services",
  "version": "1.0.7",
  "description": "",
  "main": "src/runPublicApi.js",
  "directories": {
    "test": "test"
  },
  "bin": {
    "dutchx-kovan": "./src/cli/cli-kovan.js",
    "dutchx-rinkeby": "./src/cli/cli-rinkeby.js",
    "dutchx-mainnet": "./src/cli/cli-mainnet.js"
  },
  "scripts": {
    "start": "npm run api",
    "api": "DEBUG=ERROR-*,WARN-*,INFO-*,*:BuyLiquidityBot node src/runPublicApi.js",
    "dev": "DEBUG=*-dx-service:*,*AuctionRepoImpl nodemon src/runPublicApi.js",
    "trace": "DEBUG=* nodemon src/runPublicApi.js",
    "api-rinkeby": "DEBUG=ERROR-*,WARN-*,INFO-*, PUBLIC_API_PORT=9091 NETWORK=rinkeby nodemon src/runPublicApi.js",
    "api-kovan": "DEBUG=ERROR-*,WARN-*,INFO-*, PUBLIC_API_PORT=9091 NETWORK=kovan nodemon src/runPublicApi.js",
    "bots-script": "./src/run-bots.sh",
    "bots": "DEBUG=ERROR-*,WARN-*,INFO-* node src/runBots.js",
    "bots-dev": "DEBUG=ERROR-*,WARN-*,INFO-*,DEBUG-* nodemon src/runBots.js",
    "bots-trace": "DEBUG=* node src/runBots.js",
    "bots-rinkeby": "DEBUG=ERROR-*,WARN-*,INFO-*, PUBLIC_API_PORT=9091 NETWORK=rinkeby node src/runBots.js",
    "bots-rinkeby-dev": "DEBUG=ERROR-*,WARN-*,INFO-*,DEBUG-* PUBLIC_API_PORT=9091 NETWORK=rinkeby node src/runBots.js",
    "bots-kovan": "DEBUG=ERROR-*,WARN-*,INFO-*, PUBLIC_API_PORT=9091 NETWORK=kovan node src/runBots.js",
    "bots-mainnet": "MARKETS=RDN-WETH RDN_TOKEN_ADDRESS=0x255aa6df07540cb5d3d297f0d0d4d84cb52bc8e6 OMG_TOKEN_ADDRESS=0xd26114cd6EE289AccF82350c8d8487fedB8A0C07 DEBUG=ERROR-*,WARN-*,INFO-*, PUBLIC_API_PORT=9092 NETWORK=mainnet node src/runBots.js",
    "test": "NODE_ENV=test jest",
    "test-coverage": "NODE_ENV=test jest --coverage && cat ./coverage/lcov.info | coveralls",
    "tdd": "NODE_ENV=test jest --watch",
    "test-debug": "NODE_ENV=test DEBUG=* jest",
    "cli": "DEBUG=ERROR-*,WARN-*,INFO-* node src/cli/cli.js",
    "cli-rinkeby": "DEBUG=ERROR-*,WARN-*,INFO-* NETWORK=rinkeby node src/cli/cli.js",
    "cli-kovan": "DEBUG=ERROR-*,WARN-*,INFO-* NETWORK=kovan node src/cli/cli.js",
    "cli-mainnet": "MARKETS=RDN-WETH,OMG-WETH,DAI-WETH RDN_TOKEN_ADDRESS=0x255aa6df07540cb5d3d297f0d0d4d84cb52bc8e6 OMG_TOKEN_ADDRESS=0xd26114cd6EE289AccF82350c8d8487fedB8A0C07 DAI_TOKEN_ADDRESS=0x89d24a6b4ccb1b6faa2625fe562bdd9a23260359 DEBUG=ERROR-*,WARN-*,INFO-* NETWORK=mainnet node src/cli/cli.js",
    "cli2": "DEBUG=ERROR-*,WARN-*,*-dx-service:tests*,*:DxTradeService node tests/playground/cli/bot-cli.js",
    "time": "watch -n 5 'npm run cli2 -- --time 0.1'",
    "time-slow": "watch -n 10 'npm run cli2 -- --time 0.1'",
    "time-fast": "watch -n 1 'npm run cli2 -- --time 0.2'",
    "cli2-rinkeby": "DEBUG=ERROR-*,WARN-*,*-dx-service:tests*,*:DxTradeService NETWORK=rinkeby node tests/playground/cli/bot-cli.js",
    "cli2-kovan": "DEBUG=ERROR-*,WARN-*,*-dx-service:tests*,*:DxTradeService NETWORK=kovan node tests/playground/cli/bot-cli.js",
    "cli2-debug": "DEBUG=* node tests/playground/cli/bot-cli.js",
    "data": "CACHE_ENABLED=false DEBUG=ERROR-*,WARN-*,*-dx-service:tests*,*:DxTradeService node tests/playground/cli/bot-cli.js --setup",
    "rpc": "ganache-cli -d --mnemonic 'candy maple cake sugar pudding cream honey rich smooth crumble sweet treat' --defaultBalanceEther '500000000000'",
    "truffle": "truffle",
    "migrate": "truffle migrate",
    "networks": "cd node_modules/@gnosis.pm/dx-contracts && truffle network",
    "networks-clean": "cd node_modules/@gnosis.pm/dx-contracts && truffle network --clean",
    "contracts-inject-network": "cd node_modules/@gnosis.pm/dx-contracts/ && node scripts/inject_network_info.js",
    "setup": "npm run migrate && npm run data",
    "tasks-claimFunds": "DEBUG=ERROR-*,WARN-*,INFO-* node src/tasks/claimFunds.js",
    "tasks-generateWeeklyReport": "DEBUG=ERROR-*,WARN-*,INFO-* node src/tasks/generateWeeklyReport.js",
    "tasks-generateDailyReport": "DEBUG=ERROR-*,WARN-*,INFO-* node src/tasks/generateDailyReport.js",
    "doc": "esdoc",
    "lint": "eslint .",
    "swagger": "yaml2json src/web/publicApi/static/docs/dx-public-api.yml > src/web/publicApi/static/docs/dx-public-api.json ; yaml2json src/web/bots/static/docs/api/dx-bots-api.yml > src/web/bots/static/docs/api/dx-bots-api.json"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gnosis/dx-services.git"
  },
  "license": "MIT",
  "dependencies": {
    "@gnosis.pm/dx-contracts": "^1.0.7",
    "@slack/client": "^4.8.0",
    "commander": "^2.19.0",
    "cors": "^2.8.5",
    "debug": "^3.1.0",
    "express": "^4.16.4",
    "git-rev-sync": "^1.12.0",
    "got": "^8.3.1",
    "moment": "^2.22.2",
    "node-cache": "^4.2.0",
    "qs": "^6.5.2",
    "raven": "^2.6.4",
    "truffle-contract": "^3.0.3",
    "truffle-hdwallet-provider": "0.0.6",
    "truffle-shavings": "github:okwme/arbitrage",
    "yamljs": "^0.3.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "ganache-cli": "^6.1.8",
    "jest": "^23.6.0",
    "lodash.clonedeep": "^4.5.0",
    "nodemon": "^1.18.6",
    "truffle": "4.1.5",
    "truffle-hdwallet-provider-privkey": "^0.3.0"
  }
}
